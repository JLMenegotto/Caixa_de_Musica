(defun Cx:arma_instr (#i #novo)
                               (setq instrumento_selec (Cx:instrumento #novo))
  
                               (setq *rtm_L_programa*  (subst (list #i
                                                                   (nth 1 instrumento_selec)
			                                           (nth 3 instrumento_selec)
			                                           (nth 4 instrumento_selec)
			                                     ) 
                                                             (assoc #i  *rtm_L_programa*)
				                             *rtm_L_programa*
					               )
			             *rtm_A_programa*  (subst (list #i
                                                                   (nth 2 instrumento_selec)
			                                           (nth 3 instrumento_selec)
			                                           (nth 4 instrumento_selec)
			                                     ) 
                                                             (assoc  #i  *rtm_A_programa*)
				                             *rtm_A_programa*
					               )
				    )
)
  
