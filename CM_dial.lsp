(defun Cx:campos       (#lstvar #lsttil #lstval)
                                (mapcar
			               '(lambda (var til val)
			                        (cond
						     ((= (eval var) nil)
						                       (cond
								            ((Cx:real?   var)  (set var  (atof val)))
								            ((Cx:entero? var)  (set var  (atoi val)))
								            (t                 (set var        val))
								       )     
				 	                               (set_tile til val)
						     )
                                                     (t               (cond
									   ((Cx:real?   var)  (set_tile til (rtos (eval var)  2 2)))
									   ((Cx:entero? var)  (set_tile til (itoa (eval var))))
									   (t                 (set_tile til       (eval var)))
								      )
						     )
	                    	                ) 
			               )
			                #lstvar
			                #lsttil
			                #lstval
                                )
) 

